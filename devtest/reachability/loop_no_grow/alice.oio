ALICE

channel BOB/B1@{H} [ALICE/A1;ALICE/A2;BOB/B2] : int@{H};
channel BOB/B2@{H} [ALICE/A2;BOB/B2] : int@{H};

A1@{H} [ALICE/A2;BOB/B2] (secret : int@{H}) {
    oblif secret
    then send(BOB/B2, secret+1);
    else send(BOB/B2, secret-1);
}

A2@{H} [ALICE/A2;BOB/B2] (secret : int@{H}) {
    print("ALICE/A2 received", secret);
    send(BOB/B2, secret+1);
}

{Alice}

declare salary_offer := 100<size_offer>@{Bob};
declare contract := "contract details"<size_contract>@{Bob};

init send({Bob},"OFFER",salary_offer);



hn ({Bob},"ACCEPT",x<m>) {
  print("Bob accepted, sending contract!");
  send({Bob},"CONTRACT",contract);
}

hn ({Bob},"DECLINE",x<m>) {
  print("Bob declined!");
}

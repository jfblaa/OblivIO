BOB

channel string@{H} ALICE/NAME;
channel int@{H} ALICE/AGE;
channel string@{H} ALICE/INTEREST;
channel int@{} ALICE/ASK;

START(int@{} ignored) {
  send(ALICE/NAME,"Bob");
  send(ALICE/AGE,27);
  send(ALICE/INTEREST,"fishing");
  send(ALICE/INTEREST,"cats");
  send(ALICE/INTEREST,"compilers");
  send(ALICE/ASK,0);
}

MATCH(string@{H} msg) {
  print("Alice says", msg);
}

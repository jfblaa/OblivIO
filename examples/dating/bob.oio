  send(RELAY/NAME,"Bob");
  send(RELAY/AGE,27);
  send(RELAY/INTEREST,"fishing");
  send(RELAY/INTEREST,"cats");
  send(RELAY/INTEREST,"compilers");
  send(RELAY/DONE,0);
}

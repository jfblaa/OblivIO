ALICE

channel string@{H} BOB/RCV;
input string@{H};

var string@{H} msg_out := "";

START(int@{} i : m)
!{
    oblif 0
    then msg_out ?= input(64);
    else skip;
    send(BOB/RCV, msg_out);
}
?{
    skip;
}

RCV(string@{H} msg_in : m)
!{
    skip;
}
?{
    oblif msg_in <> msg_out
    then {
        print(msg_in);
        msg_out ?= msg_in;
    }
    else msg_out ?= input(64);
    
    send(BOB/RCV, msg_out);
}
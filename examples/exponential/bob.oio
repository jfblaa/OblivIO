BOB

channel ALICE/A1@{H} $14 : int@{H};

channel ALICE/A2@{H} $2 : int@{H};

var bobs_secret : int@{H} = 0;

START@{} (i : int@{}) {
    send(ALICE/A1, bobs_secret);
}

B1@{H} $6 (secret : int@{H}) {
    oblif secret
    then send(ALICE/A2, secret+1);
    else send(ALICE/A2, secret-1);
}

B2@{H} (secret : int@{H}) {
    print("BOB/B2 received secret", secret);
}
